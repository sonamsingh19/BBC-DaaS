<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel>
			<g:HorizontalPanel spacing="10">
			<g:TextBox ui:field="keyTB"/>
			<g:TextBox ui:field="valueTB"/>
		</g:HorizontalPanel>
			<g:Cell horizontalAlignment="ALIGN_CENTER">
				<g:Button ui:field="cancelBtn" text="cancel" styleName="imageBtn"/>
			</g:Cell>
		</g:VerticalPanel>
		
	</g:HTMLPanel>
</ui:UiBinder> 